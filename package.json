{
  "name": "app",
  "module": "server/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun server/index.ts",
    "dev": "nodemon server/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "nodemon": "^3.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "bono": "^1.1.1",
    "bun": "^1.1.10",
    "fly": "^4.6.3",
    "hono": "^4.4.2",
    "zod": "^3.23.8"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,js,json",
    "ignore": [
      "node_modules"
    ],
    "exec": "bun run src/index.ts"
  }
}
